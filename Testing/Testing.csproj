<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageProjectUrl>https://github.com/danh955/Testing</PackageProjectUrl>
    <RepositoryUrl>https://github.com/danh955/Testing</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>For testing NuGet packages in GitHub.</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Version>1.0.0$(VersionSuffix)</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
